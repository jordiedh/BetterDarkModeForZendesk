{
    "manifest_version": 1,
    "name": "Better Dark Mode for Zendesk",
    "version": "1.0",
    "description": "Fixes issues with Zendesk dark mode.",
    "permissions": ["scripting"],
    "host_permissions": [
        "*://*.zendesk.com/*"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.zendesk.com/*"
        ],
        "js": ["content.js"],
        "run_at": "document_idle",
        "all_frames": true
    }]

}